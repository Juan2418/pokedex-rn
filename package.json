{
  "name": "rn-expo-boilerplate",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:staging": "NODE_ENV=staging expo start",
    "start:production": "NODE_ENV=production expo start",
    "android": "expo run:android",
    "android:staging": "NODE_ENV=staging expo run:android",
    "android:production": "NODE_ENV=production expo run:android",
    "ios": "expo run:ios",
    "ios:staging": "NODE_ENV=staging expo run:ios",
    "ios:production": "NODE_ENV=production expo run:ios",
    "clear-env-cache": "rm -rf node_modules/.cache/babel-loader",
    "web": "expo start --web",
    "prebuild": "expo prebuild --clean",
    "lint": "eslint . --fix",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "format:write": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "prepare": "husky",
    "toc": "i18next-resources-for-ts toc ./src/i18n/locales -o ./src/i18n/@types/resources.d.ts",
    "android:release-staging": "cd android && NODE_ENV=staging && ./gradlew clean bundleRelease",
    "android:release-prod": "cd android && NODE_ENV=production && ./gradlew clean bundleRelease",
    "android:apk-staging": "cd android && NODE_ENV=staging && ./gradlew clean assembleRelease",
    "android:apk-prod": "cd android && NODE_ENV=production && ./gradlew clean assembleRelease"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@tanstack/query-async-storage-persister": "^5.29.1",
    "@tanstack/react-query": "^5.29.2",
    "@tanstack/react-query-persist-client": "^5.29.2",
    "@types/uuid": "^9.0.1",
    "axios": "^1.6.8",
    "expo": "~50.0.19",
    "expo-font": "~11.10.3",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "i18next": "^23.11.4",
    "i18next-resources-for-ts": "^1.5.0",
    "intl-pluralrules": "^2.0.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-hook-form": "^7.51.3",
    "react-i18next": "^14.1.1",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-uuid": "^2.0.1",
    "tailwind-merge": "^2.2.2",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@react-native/eslint-config": "0.73.2",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "cspell": "^8.8.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "react-native-dotenv": "^3.4.11",
    "tailwindcss": "3.3.2",
    "typescript": "^5.3.0"
  },
  "private": true
}
